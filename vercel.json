{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["prisma/schema.prisma"]} 
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["GET","POST","PUT","PATCH","DELETE","OPTIONS"],
      "dest": "/api/index.js"
    }
  ]
}
